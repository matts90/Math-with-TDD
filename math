
exports.add = function(num1, num2) {
    return num1 + num2;              
};

exports.subtract = function(num1, num2) {
    return num1 - num2;              
};


exports.multiply = function(num1, num2) {
    return num1 * num2;              
};

exports.divide = function(num1, num2) {
	if (0 === num2)
	{
       	return "Cannot divide by 0";
    }
    else  
    {
    	return num1 / num2;
    }          
};